{
  "name": "@warrantyos/utl-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Universal Ticket Layer",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ticket",
    "bug-tracking",
    "warrantyos",
    "utl"
  ],
  "author": "WarrantyOS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/warrantyos/universal-ticket-layer.git",
    "directory": "sdk/typescript"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
